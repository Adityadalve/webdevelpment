CREATE TABLE transaction (
    transaction_id           BIGINT AUTO_INCREMENT PRIMARY KEY,
    trade_id_internal         VARCHAR(50),
    type                     VARCHAR(30),
    extract_date             DATE,
    transaction_datetime     DATETIME,
    transaction_day          DATE,
    unit                     VARCHAR(50),
    version                  INT,
    created_at               TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);


